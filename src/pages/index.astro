---
import Layout from '../layouts/Layout.astro';
import Track from '../components/Track.astro';

import { TRACKS } from '../mock';
---

<Layout title="MatSound">
    <ul class="flex overflow-auto">
      {TRACKS.map(track=><Track title={track.name} artist={track.artist} image={track.image} song={track.song} id={track.id} />)}
    </ul>
    
</Layout>
